# =============================================================================
# Storefront App - Environment Configuration Template
# Copy this file to .env.local and fill in your values
# =============================================================================

# Environment Mode
NEXT_PUBLIC_ENV=development

# Site URL (for SEO metadata and Open Graph)
NEXT_PUBLIC_SITE_URL=http://localhost:3004

# Auth Service API URL
NEXT_PUBLIC_AUTH_SERVICE_URL=http://localhost:3011

# GraphQL Gateway URL
NEXT_PUBLIC_GRAPHQL_URL=http://localhost:4000/graphql

# Google OAuth Client ID
NEXT_PUBLIC_GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com

# Logo URL
NEXT_PUBLIC_LOGO_URL=https://your-cdn.com/logo.png
