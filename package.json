{
  "name": "@3asoftwares/storefront-app",
  "license": "MIT",
  "version": "1.3.0",
  "description": "Customer-facing 3a Softwares storefront with product browsing, cart, and checkout",
  "scripts": {
    "dev": "next dev -p 3004",
    "build": "next build",
    "start": "next start -p 3004",
    "lint": "eslint app components lib store",
    "lint:ci": "eslint app components lib store --max-warnings 100",
    "lint:fix": "eslint app components lib store --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run:headed": "cypress run --headed",
    "cy:component": "cypress run --component",
    "cy:e2e": "start-server-and-test dev http://localhost:3004 cy:run"
  },
  "dependencies": {
    "@3asoftwares/types": "1.0.3",
    "@3asoftwares/ui": "1.0.2",
    "@3asoftwares/utils": "1.0.10",
    "@apollo/client": "^3.8.8",
    "@fortawesome/fontawesome-svg-core": "^7.1.0",
    "@fortawesome/free-solid-svg-icons": "^7.1.0",
    "@fortawesome/react-fontawesome": "^3.1.1",
    "@tanstack/react-query": "^5.90.12",
    "@tanstack/react-query-devtools": "^5.90.12",
    "axios": "^1.6.2",
    "graphql": "^16.8.1",
    "next": "^16.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recoil": "^0.7.7",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@testing-library/cypress": "^10.0.1",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.16",
    "cypress": "^13.6.0",
    "daisyui": "^4.4.19",
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.0",
    "is-number": "^7.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.32",
    "prettier": "^3.2.0",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "sharp": "^0.34.5",
    "start-server-and-test": "^2.0.3",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.0.0"
  }
}
